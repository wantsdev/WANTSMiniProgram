/* pages/WNTSShopPage/WNTSShopPage.wxss */
@import '../../utils/WNTSRecommend.wxss';
.shopDetailInfo {
  width: 750rpx;
  display: block;
}

.shopDetailInfo .containerTwoSection {
  width: 750rpx;
  display: block;
}

.shopDetailInfo .sellerDetailInfo {
  display: block;
  position: relative;
  width: 710rpx;
  height: 220rpx;
  margin-top: 20rpx;
  background: #fcfcfc;
  border-left: 20rpx solid #fff;
  border-right: 20rpx solid #fff;
  /*background-color: red;*//* overflow: hidden;
	box-shadow: 0rpx 0rpx 10rpx #f0f0f0; */
}

.shopDetailInfo .sellerBand {
  width: 120rpx;
  height: 120rpx;
  display: block;
  position: absolute;
  left: 30rpx;
  top: 50rpx;
}

.shopDetailInfo .sellerName {
  display: block;
  width: 500rpx;
  height: 44rpx;
  position: absolute;
  left: 180rpx;
  top: 0rpx;
  line-height: 220rpx;
  font-size: 32rpx;
  color: #484649;
}
.shopDetailInfo .sellerDetailBoard {
  height: 102rpx;
  width: 710rpx;
  display: block;
  position: relative;
  margin-top: 20rpx;
  background: #fcfcfc;
  border-left: 20rpx solid #fff;
  border-right: 20rpx solid #fff;
}
.shopDetailInfo .sellerDetailCall {
  width: 340rpx;
  height: 40rpx;
  border-right: 2rpx solid #f0f0f0;
  display: flex;
  flex-direction: row;
  align-items: center;
  position: absolute;
  left: 0rpx;
  top: 32rpx;
  border-radius: 0rpx;
  background: #fcfcfc;
  padding-left: 0rpx;
}
.shopDetailInfo .sellerDetailCall:after{
	border: 0rpx;
	
}
 .shopDetailInfo .intoSeller{
	width: 340rpx;
	height: 40rpx;
    display: flex;
    flex-direction: row;
	align-items: center;
	background: yellow;
    position: absolute;
	left: 340rpx;
	top: 32rpx;
	border-radius: 0rpx;
	background: #fcfcfc;
	padding-left: 0rpx;
}
.shopDetailInfo .intoSeller:after{
	border:0rpx;
}

.shopDetailInfo .intoSeller {
  width: 354rpx;
  height: 40rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  position: absolute;
  left: 354rpx;
  top: 32rpx;
}

.shopDetailInfo .sellerDetailCall .image {
  width: 28rpx;
  height: 28rpx;
  display: inline-block;
  margin-left: 96rpx;
  margin-right: 10rpx;
}

.shopDetailInfo .sellerDetailCall .imageInfo {
  width: 120rpx;
  height: 26rpx;
  line-height: 26rpx;
  display: inline-block;
  color: #484649;
  margin-left: 20rpx;
  font-size: 26rpx;
}

.shopDetailInfo .intoSeller .into {
  width: 28rpx;
  height: 28rpx;
  display: inline-block;
  margin-left: 96rpx;
  margin-right: 10rpx;
}

.shopDetailInfo .intoSeller .intoInfo {
  width: 120rpx;
  height: 26rpx;
  line-height: 26rpx;
  display: inline-block;
  color: #484649;
  margin-left: 20rpx;
  font-size: 26rpx;
}

.shopDetailInfo .guessYouLike {
  display: flex;
  flex-direction: row;
  width: 690rpx;
  height: 32rpx;
  margin-left: 30rpx;
  margin-top: 50rpx;
  /*  background: yellow;
*/
  justify-content: space-between;
}

.shopDetailInfo .guessYouLike .a {
  display: inline-block;
  font-size: 28rpx;
  width: 100%;
  height: 32rpx;
  color: #484649;
  line-height: 32rpx;
}

.shopDetailInfo .guessYouLikeDetail {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 750rpx;
  height: 100%;
  margin-top: 30rpx;
  /*  background: green;
*/
}

.shopDetailInfo .guessYouLikeItems {
  display: inline-block;
  width: 250rpx;
  height: 500rpx;
  /*  background: yellow;
*/
}

.shopDetailInfo .guessYouLikePicContainer {
  width: 250rpx;
  height: 360rpx;
  display: block;
  overflow: hidden;
  /*background-color: red;*/
}

.shopDetailInfo .guessYouLikePic {
  width: 250rpx;
  height: 400rpx;
  display: flex;
  flex-direction: row;
}

.shopDetailInfo .guessYouLikePrice {
  width: 250rpx;
  height: 110rpx;
  display: block;
  /*  
*/
  position: relative;
  overflow: hidden;
}

.shopDetailInfo .guessYouLikeIcons {
  display: inline-block;
  width: 116rpx;
  height: 40rpx;
  background: white;
  margin-right: 134rpx;
  /*  margin-bottom: 360rpx;
*/
}

.shopDetailInfo .guessYouLikeIcons .text {
  display: block;
  right: 8rpx;
  color: #fff;
  font-size: 20rpx;
  line-height: 40rpx;
}

.shopDetailInfo .guessYouLikePrice .name {
  position: absolute;
  display: block;
  width: 166rpx;
  height: 24rpx;
  /*  background: yellow;

*/
  left: 20rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.shopDetailInfo .guessYouLikePrice .changed {
  position: absolute;
  display: block;
  width: 78rpx;
  height: 32rpx;
  /*  background: blue;
*/
  bottom: 24rpx;
  left: 20rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #eb5160;
}

.shopDetailInfo .guessYouLikePrice .change {
  position: absolute;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 250rpx;
  height: 24rpx;
  /*  
*/
  bottom: 24rpx;
  right: 22rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #a8abaf;
}

.shopDetailInfo .guessYouLikePrice .change .a {
  display: block;
  width: 125rpx;
  height: 24rpx;
  /*  

*/
}

.shopDetailInfo .guessYouLikePrice .change .b {
  display: block;
  text-decoration: line-through;
}
.loading {
  width: 750rpx;
  font-size: 28rpx;
  color: #666;
  line-height: 40rpx;
  margin-bottom: 20rpx;
  text-align: center;
}