<!-- WNTSGOODSMiniProgram -->


<view wx:if="{{false}}" class="WNTSGOODSMiniProgram">
  <!--轮播图布局一⬇️ -->
  <view class="swiper-container-one" wx:if="{{(blockArrNew[index]=='BannerBlock')?(true):(false)}}">
    <swiper class="swiper" indicator-dots="{{indicatorDots}}" indicator-color="#F6F6F6" indicator-active-color="#DCDCDC" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:for="{{BannerBlock}}" wx:key="{{item.item_id}}">
        <swiper-item data-pop_item="{{item}}" catchtap="popBandClick">
          <!--src="{{item.item_image'?imageView2/2/w/800'}}"-->
          <image mode="widthFix" src="{{item.item_image}}" class="img" />
        </swiper-item>
      </block>
    </swiper>
  </view>
  <!--轮播图布局一⬆️-->

  <!-- 主题列表⬇️ -->
  <view wx:if="{{(blockArrNew[index]=='SubjectBlock')?(true):(false)}}">
    <block wx:for="{{SubjectBlock}}" wx:key="{{index}}">
    <!--主题列表⬇️布局一⬇️  -->
    <view class="detailBlockOne" wx:index="{{item.index}}" data-target="{{item.item_target.target_id}}" data-item_title="{{item.item_title}}" catchtap="band_touch">
      <view class="left">
        <text class="a" style=""> {{item.item_title}} </text>
      </view>
      <view class="right">
        <text class="a"> {{ '查看更多>' }} </text>
      </view>
    </view>
    <!--主题列表⬇️布局一⬆️  -->
    <!--主题列表⬇️布局二⬇️  -->
    <view wx:if="{{item.display_style==2}}" data-target="{{item.item_target.block_default_item_id}}" data-item_title="{{item.item_title}}" catchtap="band_touch" class="subjectCover" style="background: url({{item.item_image?item.item_image:'http://static.wantscart.com/product/1520825796736_1000_500'}}) no-repeat;background-size:cover;">
      <text class="seeMoreSubject">{{'查看更多>'}}</text>
    </view>
    <!--主题列表⬇️布局二⬆️  -->
    <scroll-view scroll-x="true" bindscrolltoupper="" bindscrolltolower="" bindscroll="" scroll-into-view="" scroll-top="" style="white-space:nowrap;" class="scrollColumn">
      <view wx:for="{{item.itemData}}" wx:for-item="itemDatas" wx:key="{{itemDatas.id}}" data-subject="{{itemDatas}}" class="subjectDetail" bindtap="columnClick">
        <image mode="scaleToFill" src="{{(itemDatas.small_img)?(itemDatas.small_img):(itemDatas.imgs[0])}}" style="{{'background:'+randomBackgroundColor[index%8]}}; margin-left:20rpx; margin-right:{{(itemDatas.index==10)?(25+'rpx'):(45+'rpx')}};" class="columnStyle">
        </image>
