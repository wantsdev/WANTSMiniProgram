        <text class="b"> {{(block_subtitle!=='')?(block_subtitle):(SubjectBlock_n_two.block_subtitle)}} </text>
      </view>
    </view>
    <view style="width:708rpx;display:flex;flex-direction:row;flex-wrap:wrap;margin-top:76rpx;margin-left:40rpx;overflow:hidden;">
      <block wx:for="{{SubjectBlock_n_two.block_items}}" wx:for-item="itemDatas" wx:key="{{index}}">
        <view data-pop_item="{{itemDatas}}" catchtap="popBandClick" class="subjectDetail" style="width:314rpx;height:430rpx;display:inline-block;margin-bottom:30rpx;margin-right:{{(index%2==0)?(0+'rpx'):(40+'rpx')}}; margin-left:{{(index%2==1)?(40+'rpx'):(0+'rpx')}};">
