/**轮播图**/

.swiper-container-one {
  position: relative;
  height: 376rpx;
}

.swiper-container-one .swiper {
  height: 376rpx;
  background-color: gainsboro;
}

.swiper-container-one .swiper .img {
  width: 100%;
  height: 100%;
}

.swiper-container-one .dots {
  position: absolute;
  right: 290rpx;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
}

.swiper-container-one .dots .dot {
  margin: 0 10rpx;
  width: 15rpx;
  height: 15rpx;
  background: #fff;
  border-radius: 50%;
  transition: all 0.6s;
  font: 300 0rpx/0rpx "microsoft yahei";
  text-align: center;
}

.swiper-container-one .dots .dot.active {
  background: yellow;
  color: yellow;
}

.swiper-container-one .dots.on {
  position: absolute;
  right: 290rpx;
  bottom: 20rpx;
  display: flex;
  color: red;
  justify-content: center;
}

.swiper-container-one .dots .dot {
  margin: 0 10rpx;
  width: 15rpx;
  height: 15rpx;
  background: #fff;
  border-radius: 50%;
  transition: all 0.6s;
  font: 300 0rpx/0rpx "microsoft yahei";
  text-align: center;
}

.swiper-container-one .dots .dot.active {
  background: yellow;
  color: yellow;
}

.swiper-container-one .dots.on {
  position: absolute;
  right: 290rpx;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
}
